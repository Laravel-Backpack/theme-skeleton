            ":uc:vendor\\:uc:package\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "laravel": {
            "providers": [
                ":uc:vendor\\:uc:package\\AddonServiceProvider"
            ]
        }
    }
}
